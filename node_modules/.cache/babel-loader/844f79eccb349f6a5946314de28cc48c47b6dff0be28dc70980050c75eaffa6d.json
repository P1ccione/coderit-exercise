e\n      }\n    },\n    created() {\n      this.teachers\n    },\n    methods: {\n      async fetchTeachers() {\n        const res = await fetch(\"http://localhost:5000/teacher\")\n\n        const data = await res.json()\n      },\n      toggleCreateTeacher() {\n        this.showCreateTeacher = !this.showCreateTeacher\n      },\n      async addTask(teacher) {\n        const res = await fetch(\"http://localhost:5000/teacher\", {\n            method: \"POST\",\n            headers: {\"Content-Type\": \"application/json\"},\n            body: JSON.stringify(teacher),\n        })\n\n        const data = await res.json()\n\n        this.teacher = [...this.teacher, data]\n      },\n    }\n  }\n</script>\n<style>\n  @import url('https://fonts.googleapis.com/css2?family=Poppins:wght@300;400&display=swap');\n    \n  * {\n    box-sizing: border-box;\n    margin: 0;\n    padding: 0;\n    /* outline: 1px orange solid */\n  }\n\n  body {\n    font-family: 'Poppins', sans-serif;\n  }\n\n  .container {\n    width: 100%;\n    height: 100vh;\n    display: flex;\n    flex-direction: column;\n  }\n\n  .content-container {\n    width: 100%;\n    height: 93vh;\n    display: flex;\n  }\n\n</style>\n"],"mappings":"AAWE,OAAOA,MAAK,MAAO,qBAAoB;AACvC,OAAOC,OAAM,MAAO,sBAAqB;AACzC,OAAOC,UAAS,MAAO,yBAAwB;AAE/C,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVJ,MAAM;IACNC,OAAO;IACPC;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,iBAAiB,EAAE;IACrB;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACF,QAAO;EACd,CAAC;EACDG,OAAO,EAAE;IACP,MAAMC,aAAaA,CAAA,EAAG;MACpB,MAAMC,GAAE,GAAI,MAAMC,KAAK,CAAC,+BAA+B;MAEvD,MAAMP,IAAG,GAAI,MAAMM,GAAG,CAACE,IAAI,CAAC;IAC9B,CAAC;IACDC,mBAAmBA,CAAA,EAAG;MACpB,IAAI,CAACP,iBAAgB,GAAI,CAAC,IAAI,CAACA,iBAAgB;IACjD,CAAC;IACD,MAAMQ,OAAOA,CAACC,OAAO,EAAE;MACrB,MAAML,GAAE,GAAI,MAAMC,KAAK,CAAC,+BAA+B,EAAE;QACrDK,MAAM,EAAE,MAAM;QACdC,OAAO,EAAE;UAAC,cAAc,EAAE;QAAkB,CAAC;QAC7CC,IAAI,EAAEC,IAAI,CAACC,SAAS,CAACL,OAAO;MAChC,CAAC;MAED,MAAMX,IAAG,GAAI,MAAMM,GAAG,CAACE,I